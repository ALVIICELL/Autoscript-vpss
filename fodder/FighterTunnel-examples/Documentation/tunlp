GITHUB_CMD="https://github.com/rullpqh/Autoscript-vps/raw/main/fodder/FighterTunnel-examples/Documentation/"
apt update -y  >/dev/null 2>&1
wget -O /etc/systemd/system/limitvmess.service "${GITHUB_CMD}1" >/dev/null 2>&1
wget -O /etc/systemd/system/limitvless.service "${GITHUB_CMD}2" >/dev/null 2>&1
wget -O /etc/systemd/system/limittrojan.service "${GITHUB_CMD}3" >/dev/null 2>&1
wget -O /etc/systemd/system/limitshadowsocks.service "${GITHUB_CMD}4" >/dev/null 2>&1
wget -O /usr/bin/limit.vmess "${GIT_CMD}vmess" && chmod +x limit.vmess >/dev/null 2>&1
wget -O /usr/bin/limit.vless "${GIT_CMD}vless" && chmod +x limit.vless >/dev/null 2>&1
wget -O /usr/bin/limit.trojan "${GIT_CMD}trojan" && chmod +x limit.trojan >/dev/null 2>&1
wget -O /usr/bin/limit.shadowsocks "${GIT_CMD}shadowsocks" && chmod +x limit.shadowsocks >/dev/null 2>&1
systemctl daemon-reload
systemctl enable limitvmess.service
systemctl enable limitvless.service
systemctl enable limittrojan.service
systemctl enable limitshadowsocks.service
systemctl start limitvmess.service
systemctl start limitvless.service
systemctl start limittrojan.service
systemctl start limitshadowsocks.service
systemctl restart limitvmess.service
systemctl restart limitvless.service
systemctl restart limittrojan.service
systemctl restart limitshadowsocks.service
